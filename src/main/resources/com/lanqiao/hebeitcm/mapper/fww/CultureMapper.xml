<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanqiao.hebeitcm.mapper.fww.CultureMapper">

    <resultMap id="BaseResultMap" type="com.lanqiao.hebeitcm.model.fww.culturals">
        <id property="itemID" column="itemID"/>
        <result property="itemCode" column="itemCode" jdbcType="CHAR"/>
        <result property="culturalname" column="CHINESE_CULTURAL_NAME"/>
        <result property="culturalcontent" column="CHINESE_CULTURAL_CONTENT"/>
        <result property="culturalsource" column="CHINESE_CULTURAL_SOURCE"/>
        <result property="culturalauthor" column="CHINESE_CULTURAL_AUTHOR"/>
        <result property="visitnum" column="VISIT_NUM"/>
        <result property="culturaltype" column="CHINESE_CULTURAL_TYPE"/>
        <result property="culturalstatus" column="CHINESE_CULTURAL_STATUS"/>
        <result property="creater" column="CREATER"/>
        <result property="itemCreateAt" column="itemCreateAt" />
        <result property="updater" column="UPDATER"/>
        <result property="itemupdateAt" column="itemUpdateAt" />
    </resultMap>

<select id="selectAllFilms" resultMap="BaseResultMap">
  select
      itemID,
      itemCode,
      CHINESE_CULTURAL_NAME,
      CHINESE_CULTURAL_CONTENT,
      CHINESE_CULTURAL_SOURCE,
      CHINESE_CULTURAL_AUTHOR,
      VISIT_NUM,
      CHINESE_CULTURAL_TYPE,
      CHINESE_CULTURAL_STATUS,
      CREATER,
      itemCreateAt,
      UPDATER,
      itemUpdateAt
  from tb_culpro_chinese_cultural
  where CHINESE_CULTURAL_NAME like "%电视电影%"
  order by VISIT_NUM DESC
</select>


</mapper>
